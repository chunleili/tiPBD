{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "profile",
            "type": "debugpy",
            "request": "launch",
            "module": "cProfile",
            "justMyCode": true,
            "args":["-o",
                    "${fileBasenameNoExtension}.prof",
                    "${file}"]
        },
        {
            "name": "Python C++ Debug",
            "type": "pythoncpp",
            "request": "launch",
            "pythonLaunchName": "Python: Current File",
            "cppAttachName": "(Windows) Attach",
          },
          {
            "name": "(Windows) Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": ""
          },
          {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
              "-use_graph_coloring=1",
              "-use_detailed_smoothers=1",
              "-presmoother_type_lv","gauss_seidel", "jacobi",
              "-presmoother_niter_lv","2","1"
            ]
          }
    ]
}